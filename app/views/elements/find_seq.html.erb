<%=form_with url: '/elements/res_of_search', method: :get do |form|%>
    <div class = 'form-group' style = 'width: 60%;'>
      <%= form.label :sequence%>
      <%= form.text_field(:str_seq, {:class => 'form-control'})%> 
    </div>

    <div class = 'form-group' style = 'width: 60%; margin-top: 10px;'>
      <%= form.submit "search", class: 'btn btn-primary' %>
      <%=link_to 'Go home', home_path%>
    </div>
  <%end%>